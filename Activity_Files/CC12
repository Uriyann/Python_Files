for x in range(11, 0, -1):
      for z in range(1, x + 1):
            print(" ", end=" ")

      for y in range(11, x, -1):
            print("*", end= " ")

      for w in range(11, x, -1):
            print("*", end= " ")
      print()

for a in range(1, 11):
      for b in range(1, 10):
            print(" ", end=" ")
      for c in range(1, 5):
            print("*", end= " ")
      print()