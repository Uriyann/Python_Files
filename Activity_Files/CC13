for x in range(1, 5):
      for z in range(5, x, - 1):
            print(" ", end=" ")

      for y in range(x, 1, -1):
            print(y, end= " ")

      for w in range(1, x+1):
            print(w, end= " ")

      print()

for x in range(5, 0, -1):
      for z in range(5, x, - 1):
            print(" ", end=" ")

      for y in range(x, 1, -1):
            print(y, end= " ")

      for w in range(1, x+1):
            print(w, end= " ")

      print()