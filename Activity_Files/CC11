for x in range(11, 0, -1):
      for z in range(1, x + 1):
            print(" ", end=" ")

      for y in range(11, x, -1):
            print("*", end= " ")

      for w in range(10, x, -1):
            print("*", end= " ")
      print()

for x in range(1, 11):
      for z in range(1, x + 2):
            print(" ", end=" ")

      for y in range(10, x, -1):
            print("*", end= " ")

      for w in range(9, x, -1):
            print("*", end= " ")
      print()